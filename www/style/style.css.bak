body {
    background-color: #ffffff;
    color: black;
    width: 100%;
}
footer {
	position: fixed;
    margin-left: 0;
    margin-right: 0;
    padding-left: 0;
    padding-right: 0;
	bottom: 0;
    width: 100%;
    max-width: 2048px;
	text-align:center;
	display: block;
	align-items: center !important;
	flex-direction: row-reverse;
}
.navbar {
  background-color: #00422e; /* Replace with your desired background color */
}
.navbar > li > a {margin-top:50px;width: 100%;}
.navbar-default {
    background-color: #00422e !important;
    border-color: #00422e !important;
}
.navbar-default .navbar-brand {
    color: white;
}
.br { border: none; }
.navbar-default .navbar-nav > .active > a {
        background-color: #ffffff !important; /* Replace with desired color */
        color: #000000 !important; /* Replace with desired text color */
    }

/*.navbar-nav > li:first-child { visibility: hidden;}*/
.navbar-nav > li:first-child { display: none !important; } /* To hide the first child */
.navbar-nav > li:nth-child(3) { display: none !important; } /* To hide the nth child */
.nav>li>a {
    padding-top: 23px;
    padding-bottom: 23px;
    margin-top:5px;
    margin-bottom:-2px;
}
.nav-tabs>li>a {
    color: white;
    background-color: black;
    border: 1px solid black;
    border-radius: 0;
}
.nav-tabs>li>a:hover {
    background-color: #ffffff !important;
    border-color: #e0e0e0 !important;
    color: #000000 !important;
    /*box-shadow: inset 0 0 100px 100px rgba(27, 27, 27);*/
}
.nav-tabs>.active>a {
    background-color: #00422e;
    border-color: #00422e;
    color: #ffffff;
}
          
.nav-tabs > li[class=active] > a {
    background-color: #00422e;
    color: #ffffff;
}
.navbar-default .navbar-nav>li>a:hover {
  color: White;
}

.navbar-default .navbar-brand:hover {
  color: white;
}
.hero {
  display: flex;
  justify-content: center;
}
.header {
  display: flex;
  align-items: center !important;
  flex-direction: row-reverse;
}
.row {
  display: flex;
  align-items: center;
}
.social-btn {
  border-radius: 100%;
  width: 40px;
  height: 40px;
  border: none;
}
.social-bar {
  height: 100px;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.featurette-heading {
  font-weight: 900;
}
.row-reverse {
  display: flex;
  align-items: center;
  flex-direction: row-reverse;
}
.feature-rows {
  display: flex;
  justify-content: space-around;
  align-items: center;
  align-content: center;
  padding: 5vw;
}
.spacer {
  height: 10px;
}
.img{
        max-width: 100%;
        max-height: 100%;
}

.navbar img {
  max-width: 100%; /* Initially, the image takes 100% of the navbar width */
}

